            "name": "Cristian Moreno",
            "email": "amengels1@gmail.com"
        },
        {
            "name": "Edgardo Troche",
            "email": "mega3087@hotmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.*",
        "cartalyst/sentry": "2.1.*",
        "graham-campbell/binput": "1.1.*@dev",
        "mcamara/laravel-localization": "0.13.*",
        "intervention/image": "1.*"
    },
    "require-dev": {
        "illuminate/database": "4.1.*"
    },
    "suggest": {
        "cartalyst/sentry": "Autentificacion de Usuarios",
        "graham-campbell/binput": "Seguridad en Datos",
        "mcamara/laravel-localization": "18n Multiplataforma",
        "intervention/image": "Manipulacion de Imagenes"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "src/controllers",
            "src/models",
            "src/commands"
        ],
        "psr-0": {
            "Ecco\\Ecsa\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "stable"
}
